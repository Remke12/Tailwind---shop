  <!-- Your palette -->
  <script>
    tailwind.config = {
      theme: {
        extend: {
          colors: {
            dust: "#d7cdcc",
            slateblue: "#59656f",
            grape: "#9c528b",
            shadow: "#1d1e2c",
            pure: "#ffffff",
          }
        }
      }
    }
  </script>
